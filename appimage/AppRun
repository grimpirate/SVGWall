#!/bin/sh

DIR="$(dirname "$(readlink -f "${0}")")"
exec "$DIR/jre/bin/java" -Dawt.useSystemAAFontSettings=on -Dswing.aatext=true -Dfile.encoding=UTF-8 -Dstdout.encoding=UTF-8 -Dstderr.encoding=UTF-8 -Djava.library.path="$DIR/../lib" -classpath "$DIR:$DIR/../lib/picocli-4.7.6.jar:$DIR/../lib/xmlgraphics-commons-2.9.jar:$DIR/../lib/batik-anim-1.17.jar:$DIR/../lib/batik-bridge-1.17.jar:$DIR/../lib/batik-css-1.17.jar:$DIR/../lib/batik-dom-1.17.jar:$DIR/../lib/batik-gvt-1.17.jar:$DIR/../lib/batik-transcoder-1.17.jar:$DIR/../lib/batik-util-1.17.jar:$DIR/../lib/batik-i18n-1.17.jar:$DIR/../lib/batik-svg-dom-1.17.jar:$DIR/../lib/batik-constants-1.17.jar:$DIR/../lib/batik-ext-1.17.jar:$DIR/../lib/batik-xml-1.17.jar:$DIR/../lib/batik-parser-1.17.jar:$DIR/../lib/batik-script-1.17.jar:$DIR/../lib/batik-awt-util-1.17.jar:$DIR/../lib/xml-apis-ext-1.3.04.jar" com.grimpirate.SVGWall -m "$DIR/../share/metainfo/svgwall.appdata.xml" "$@"
